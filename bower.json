{
    "name": "fcoo-maps-marker",
    "version": "1.1.3",
    "homepage": "https://github.com/FCOO/fcoo-maps-marker",
    "authors": [
        "Niels Holt"
    ],
    "description": "",
    "main": [
        "dist/fcoo-maps-marker.js",
        "dist/fcoo-maps-marker.css"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/FCOO/fcoo-maps-marker.git"
    },
    "dependencies": {
        "fcoo-maps": "fcoo/fcoo-maps#latest",
        "leaflet-bootstrap-marker": "fcoo/leaflet-bootstrap-marker#latest"
    },
    "devDependencies": {
        "bourbon": "latest",
        "modernizr-mixin": "latest",
        "mathsass": "latest"
    },
    "license": "MIT",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "moduleType": [
        "es6"
    ],
    "overrides": {
        "leaflet.locatecontrol": {
            "main": [
                "src/L.Control.Locate.js"
            ]
        },
        "modernizr-mouse-events": {
            "dependencies": {
                "fcoo-modernizr-mediaquery-device": "latest"
            }
        },
        "offline": {
            "main": "offline.js"
        },
        "contactjs": {
            "main": "dist/contact.js"
        },
        "swiped-events": {
            "main": "src/swiped-events.js"
        },
        "bootstrap-select": {
            "main": [
                "dist/js/bootstrap-select.js"
            ]
        },
        "jquery.scrollbar": {
            "main": [
                "jquery.scrollbar.js"
            ]
        },
        "bootstrap": {
            "dependencies": {
                "jquery": "latest"
            },
            "main": [
                "dist/js/bootstrap.bundle.js"
            ]
        },
        "inputmask": {
            "main": [
                "dist/jquery.inputmask.bundle.js"
            ]
        },
        "js-url": {
            "main": [
                "dist/url.min.js"
            ]
        },
        "moment": {
            "main": [
                "moment.js",
                "locale/da.js",
                "locale/de.js",
                "locale/sv.js",
                "locale/nb.js",
                "locale/fo.js"
            ]
        },
        "mmenu-js": {
            "main": "dist/mmenu.js"
        },
        "leaflet": {
            "main": [
                "dist/leaflet-src.js",
                "dist/leaflet.css"
            ],
            "dependencies": {
                "leaflet-global-switches": "latest"
            }
        },
        "Leaflet.GeometryUtil": {
            "main": "src/leaflet.geometryutil.js",
            "dependencies": {
                "leaflet": "latest"
            }
        },
        "Leaflet.Geodesic": {
            "dependencies": {
                "leaflet": "latest"
            }
        }
    },
    "resolutions": {
        "leaflet-bugfix": "3.0.0",
        "bootstrap": "^4.0.0",
        "leaflet": "1.7.1"
    }
}